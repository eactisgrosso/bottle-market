{
  "name": "@bottle-market/api",
  "version": "2.0.2",
  "scripts": {
    "shop": "AWS_PROFILE=bottlehub ts-node-dev --inspect --respawn server.shop.ts",
    "admin": "AWS_PROFILE=bottlehub ts-node-dev --inspect --respawn server.admin.ts",
    "retailer": "AWS_PROFILE=bottlehub ts-node-dev --inspect --respawn server.retailer.ts",
    "build": "rm -rf dist && mkdir dist && tsc -b && copyfiles *.gql dist/",
    "postbuild": "cp package.json dist/package.json && cd dist && npm install --only=production",
    "shop-serve": "NODE_ENV=production node dist/server.shop.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.9",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/graphql": "^7.3.4",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-fastify": "^7.0.9",
    "@nestjsplus/knex": "^1.0.0",
    "apollo-server-fastify": "^2.12.0",
    "aws-serverless-fastify": "^1.0.13",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "fuse.js": "5.0.1-beta",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0",
    "knex": "^0.21.1",
    "knex-postgis": "^0.12.0",
    "lodash": "^4.17.15",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.1.3",
    "@types/aws-lambda": "^8.10.51",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.31",
    "@types/passport-jwt": "^3.0.3",
    "aws-sdk": "^2.671.0",
    "copyfiles": "^2.2.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "license": "ISC"
}
